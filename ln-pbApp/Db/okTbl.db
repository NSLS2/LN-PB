#
# Wrap into +-180 deg
#
  field(DESC,    "Physics setpoint:")
  field(DESC,    "Ref phase(L) +-360 deg")
  field(CALC,    "(A<B)?(A+D):((A>C)?(A-D):A)")
  field(INPA,    "$(S1){$(D1)-$(TBL)}Phs:Nrm-Calc")
  field(INPB,    "-180.0")
  field(INPC,    "+180.0")
  field(INPD,    "+360.0")
  field(OUT,     "$(S1){$(D1)-$(TBL)}Phs:Val-SP PP")
}

