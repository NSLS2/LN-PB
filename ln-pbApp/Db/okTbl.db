  field(DESC,    "+-360 = ")
  field(PREC,    "1")
  field(EGU,     "deg")
  field(DRVH,    "+180.0")
  field(HOPR,    "+180.0")
  field(DRVL,    "-180.0")
  field(LOPR,    "-180.0")
}

