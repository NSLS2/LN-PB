record(ao,       "$(S1){$(D1)-$(TBL)}E:Fb-SP") {
  field(DESC,    "$(TBL) Flat bottom SP E")
  field(PREC,    "3")
  field(DRVH,    "1")
  field(DRVL,    "0")
  field(HOPR,    "1")
  field(LOPR,    "0")
  field(PINI,    "YES")
  info(autosaveFields_pass0, "VAL")
}

record(ao,       "$(S1){$(D1)-$(TBL)}Phs:Fb-SP") {
  field(DESC,    "$(TBL) Flat bottom SP Phs")
  field(PREC,    "1")
  field(DRVH,    "360")
  field(DRVL,    "-360")
  field(HOPR,    "360")
  field(LOPR,    "-360")
  field(EGU ,    "deg")
  field(PINI,    "YES")
  info(autosaveFields_pass0, "VAL")
}

